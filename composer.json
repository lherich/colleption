{
    "name": "lherich/colleption",
    "description": "A collection of common PHP exception classes. Edit",
    "keywords": ["library", "collection", "exception", "exceptions"],
    "version": "1.0.0",
    "license": "MIT",
    "homepage": "https://github.com/lherich/colleption",
    "support": {
        "issues": "https://github.com/lherich/colleption/issues",
        "source": "https://github.com/lherich/colleption"
    },
    "authors": [
        {
            "name": "Lucas Herich",
            "email": "colleption@herich.net"
        }
    ],
    "require": {
        "php": ">=5.6.4"
    },
    "autoload": {
        "psr-4": {
            "Colleption\\": "src/Colleption/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1"
    },
    "autoload-dev": {
        "psr-4": {
